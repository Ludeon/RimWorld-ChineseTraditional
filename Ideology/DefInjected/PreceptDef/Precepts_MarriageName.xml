<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: always man's -->
  <MarriageName_AlwaysMans.label>冠夫姓</MarriageName_AlwaysMans.label>
  <!-- EN: Couples must share the man's name. -->
  <MarriageName_AlwaysMans.description>妻隨夫姓。</MarriageName_AlwaysMans.description>
  
  <!-- EN: always woman's -->
  <MarriageName_AlwaysWomans.label>冠妻姓</MarriageName_AlwaysWomans.label>
  <!-- EN: Couples must share the woman's name. -->
  <MarriageName_AlwaysWomans.description>夫隨妻姓。</MarriageName_AlwaysWomans.description>
  
  <!-- EN: keep names -->
  <MarriageName_KeepNames.label>不改姓</MarriageName_KeepNames.label>
  <!-- EN: Both partners keep their names upon marriage. -->
  <MarriageName_KeepNames.description>夫妻各自保留姓氏。</MarriageName_KeepNames.description>
  
  <!-- EN: random -->
  <MarriageName_Random.label>隨機</MarriageName_Random.label>
  <!-- EN: Couples randomly choose whether to share a name, and which name to share. -->
  <MarriageName_Random.description>隨機決定夫妻是否改姓，及誰隨誰姓。</MarriageName_Random.description>
  
  <!-- EN: usually woman's -->
  <MarriageName_UsuallyWomans.label>常冠妻姓</MarriageName_UsuallyWomans.label>
  <!-- EN: Most couples share the woman's name. -->
  <MarriageName_UsuallyWomans.description>通常夫隨妻姓。</MarriageName_UsuallyWomans.description>
  
</LanguageData>