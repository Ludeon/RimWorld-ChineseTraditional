  <!-- EN: pop firefoam -->
  <Apparel_FirefoampopPack.verbs.pop_firefoam.label>噴發泡沫</Apparel_FirefoampopPack.verbs.pop_firefoam.label>
  
  <!-- EN: smokepop pack -->
  <Apparel_SmokepopBelt.label>煙罐包</Apparel_SmokepopBelt.label>
